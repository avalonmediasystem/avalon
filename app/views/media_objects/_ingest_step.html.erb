<li class="<% if active %>nav-success<% else %>nav-disabled<% end %>"> 
  <span class="step"><%= @workflow_steps.index(step.step) %></span>
    <% if active %>
      <%= link_to edit_media_object_path(@mediaobject, step: step.template) do %>
        <h4><%= step.title %></h4>
      <% end %>
    <% else %>
      <h4><%= step.title %></h4>
    <% end %>
    <p><%= step.summary %></p>
</li>
